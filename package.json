{
  "name": "electronjs-website",
  "version": "1.0.0",
  "description": "The website for Electron",
  "repository": "https://github.com/Akshit1025/electronjs-website",
  "license": "MIT",
  "author": "Akshit Gupta",
  "scripts": {
    "start": "cross-env NODE_OPTIONS='--max_old_space_size=4096' NODE_PATH=. NODE_ENV=production node server.js",
    "bootstrap": "node script/bootstrap",
    "test": "cross-env NODE_PATH=. NODE_ENV=test mocha --timeout 5000 && eslint . --fix",
    "integration": "script/integration.sh",
    "release": "node script/release",
    "dev": "cross-env NODE_PATH=. NODE_ENV=development nodemon server.js",
    "prepack": "check-for-leaks",
    "precompile-assets": "rimraf precompiled && cross-env NODE_ENV=production webpack --config ./webpack.common.js && node script/precompile-assets.js",
    "linkschecker": "NODE_PATH=. NODE_ENV=test node scripts/links-checker.js",
    "cypress": "cypress run",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint . --fix",
    "lint:style": "stylelint 'public/styles/**/*.scss' --fix",
    "heroku-postbuild": "npm run precompile-assets",
    "update-data": "node script/update-data.js"
  },
  "dependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@primer/css": "^20.2.4",
    "@primer/octicons": "^17.3.0",
    "awesome-electron": "^2.6.0",
    "browser-date-formatter": "^3.0.3",
    "cheerio": "*",
    "clipboard": "^2.0.11",
    "compression": "^1.7.4",
    "connect-browser-sync": "^2.1.0",
    "connect-slashes": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "crowdin-editor-language-codes": "^1.0.0",
    "description": "^0.0.4",
    "dotenv": "^16.0.1",
    "electron-api-historian": "^1.94.0",
    "electron-apps": "^1.9312.0",
    "electron-i18n": "^1.3136.0",
    "electron-markdown": "^0.9.0",
    "electron-releases": "^3.1072.0",
    "electron-userland-reports": "^1.6.0",
    "express": "^4.18.1",
    "express-hbs": "^2.4.0",
    "express-request-language": "^1.1.15",
    "express-useragent": "^1.0.15",
    "fast-memoize": "^2.5.2",
    "feed": "^4.2.2",
    "flat": "^5.0.2",
    "fs-extra": "^10.1.0",
    "got": "^12.1.0",
    "gray-matter": "^4.0.3",
    "helmet": "^5.1.0",
    "http-proxy-middleware": "^2.0.6",
    "instantsearch.js": "^4.43.1",
    "lil-env-thing": "^1.0.0",
    "lobars": "^1.2.0",
    "locale-code": "^2.0.2",
    "lodash": "^4.17.21",
    "lunr": "^2.3.9",
    "make-promises-safe": "^5.1.0",
    "minimist": "^1.2.6",
    "node-sass-middleware": "^1.0.1",
    "paginate-array": "^2.1.0",
    "platform-utils": "^1.2.0",
    "pluralize": "^8.0.0",
    "query-string": "^7.1.1",
    "require-dir": "^1.2.0",
    "require-directory": "^2.1.1",
    "require-yaml": "^0.0.1",
    "rtl-detect": "^1.0.4",
    "schemeless": "^1.2.0",
    "search-with-your-keyboard": "^2.0.0",
    "semver": "^7.3.7",
    "set-query-string": "^2.2.0",
    "tocbot": "^4.18.2",
    "ultimate-pagination": "^1.0.0"
  },
  "devDependencies": {
    "@octokit/action": "^4.0.4",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "babel-loader": "^8.2.5",
    "browser-sync": "^2.27.10",
    "chai": "^4.3.6",
    "chai-cheerio": "^1.0.0",
    "check-for-leaks": "^1.2.1",
    "cypress": "^10.3.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "globby": "^13.1.2",
    "handlebars-loader": "^1.7.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "nock": "^13.2.8",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "simplecrawler": "^1.1.9",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-scss": "^4.3.0",
    "supertest": "^6.2.4",
    "supertest-session": "^4.1.0",
    "wait-on": "^6.0.1",
    "walk-sync": "^3.0.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-middleware": "^5.3.3",
    "webpack-manifest-plugin": "^5.0.0"
  },
  "lint-staged": {
    "*.js": "npm run lint:js",
    "*.scss": "npm run lint:style"
  },
  "resolutions": {
    "node-sass-middleware/node-sass": "^6.0.1"
  },
  "husky": {
    "hooks": {
      "pre-push": "check-for-leaks",
      "pre-commit": "lint-staged"
    }
  }
}
